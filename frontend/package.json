{
  "name": "galaxy-erp-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:standalone": "next build",
    "build:export": "next build && next export",
    "build:static": "NEXT_CONFIG=static next build",
    "export:static": "NEXT_CONFIG=static next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "preview:static": "npx serve out",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "clean:static": "rm -rf .next out && npm run build:static"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.13.0",
    "dayjs": "^1.11.18",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "typescript": "^5.4.0"
  }
}
